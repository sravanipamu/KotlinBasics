<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/variable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Variable"
        android:onClick="variable"
        android:layout_marginTop="100dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/null_safty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Null Safty"
        android:onClick="NullSafty"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/variable"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
         />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/const_keyword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Const Keyword"
        android:layout_marginTop="20dp"
        android:onClick="constKeyword"
        app:layout_constraintTop_toBottomOf="@+id/null_safty"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/lateinit_lazy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lateinit and Lazy"
        android:layout_marginTop="20dp"
        android:onClick="lateinitlazy"
        app:layout_constraintTop_toBottomOf="@+id/const_keyword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/visibility_modifier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Visibility Modifier"
        android:onClick="visibility"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/lateinit_lazy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/lamda_highorder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lamda HighOrder"
        android:onClick="lamdaHighOrder"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/visibility_modifier"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/object"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Object"
        android:onClick="objectCompanionObject"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/lamda_highorder"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/inline_functions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inline Functions"
        android:onClick="inlineFunctions"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/object"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/data_class"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Class"
        android:onClick="dataClass"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/inline_functions"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>